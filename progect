import sys
import math

number=     #number는 규범님께서 담당하신 부분에서 구하신 숫자입니다. 
def get_prime(n):
    is_prime=[]
    for i in range(2,n+1):
        if i==2:
	    is_prime.append(i)
	else:
	    for j in range(2,n+2):
	        if i%j!=0 and j==n+1:
		    is_prime.append(i)
		    break
		if i%j==0:
		    break
print(is_prime)
	    


